      def self.execute: (Array[String] args, Hash[Symbol, untyped] options, Hash[Symbol, untyped] global_options) -> Integer

      def initialize: (Array[String] args, Hash[Symbol, untyped] options, Hash[Symbol, untyped] global_options) -> void

      def execute: () -> Integer

      private

      def perform_clone: (Integer ctid, Hash[Symbol, untyped] config_params) -> Integer

      def print_progress: (untyped result) -> void

      def load_config: () -> void

      def service_options: () -> Hash[Symbol, untyped]

      def output_result: (untyped result) -> void

      def display_clone_summary: (Integer ctid, Hash[Symbol, untyped] config_params) -> Symbol?

      def display_config_changes: (Hash[Symbol, untyped] params) -> void

      def usage_error: (String message) -> Integer
    end
  end
end
