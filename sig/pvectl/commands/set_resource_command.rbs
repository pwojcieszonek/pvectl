module Pvectl
  module Commands
    module SetResourceCommand
      module ClassMethods
        def execute: (Array[String] args, Hash[Symbol, untyped] options, Hash[Symbol, untyped] global_options) -> Integer
      end

      def self.included: (Class base) -> void

      def initialize: (Array[String] args, Hash[Symbol, untyped] options, Hash[Symbol, untyped] global_options) -> void
      def execute: () -> Integer

      private
      def resource_label: () -> String
      def resource_id_label: () -> String
      def execute_params: (String resource_id, Hash[String, String] key_values) -> Hash[Symbol, untyped]
      def build_set_service: (untyped connection) -> untyped
      def parse_key_values: (Array[String]? args) -> Hash[String, String]
      def display_diff: (untyped result) -> void
      def load_config: () -> void
      def usage_error: (String message) -> Integer
      def service_options: () -> Hash[Symbol, untyped]
    end
  end
end
