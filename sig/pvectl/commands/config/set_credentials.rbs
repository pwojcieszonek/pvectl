module Pvectl
  module Commands
    module Config
      class SetCredentials
        def self.register_subcommand: (untyped parent) -> void

        def self.execute: (String user_name, Hash[Symbol, untyped] options, Hash[Symbol, untyped] global_options) -> Integer

        def self.validate_new_user_credentials: (String? token_id, String? token_secret, String? username, String? password) -> Integer?

        def self.validate_partial_update: (Hash[Symbol, untyped] options, untyped existing_user) -> Integer?
      end
    end
  end
end
