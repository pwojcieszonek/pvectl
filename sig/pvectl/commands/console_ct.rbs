module Pvectl
  module Commands
    class ConsoleCt
      def self.execute: (String? ctid, Hash[Symbol, untyped] options, Hash[Symbol, untyped] global_options) -> Integer

      def initialize: (String? ctid, Hash[Symbol, untyped] options, Hash[Symbol, untyped] global_options) -> void

      def execute: () -> Integer

      private

      def resource_path: () -> String

      def resolve_credentials: () -> [String?, String?]

      def prompt_username: (?String? default) -> String?

      def prompt_password: () -> String?

      def load_config: () -> void

      def usage_error: (String message) -> Integer

      def not_found: (String message) -> Integer
    end
  end
end
